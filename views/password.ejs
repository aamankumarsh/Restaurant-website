<%- include("links") -%>
<!-- Linking The Custom CSS -->
<link href="signin.css" rel="stylesheet" />
<title>Login In</title>
</head>

<body>
<form action="/password" class="form-signin" method="post">
  <img src="logo.jpg" alt="Prime Location Logo" width="200" height="200" />
  <h1>Forgotten Password</h1>
  <input
    type="email"
    class="form-control"
    name="email"
    placeholder="Email address"
    required
    autofocus
  />
  <button class="btn btn-lg btn-primary btn-block" type="submit">
    Reset Password
  </button>
  <%- include("co") -%>
</form>
</body>
</html>
